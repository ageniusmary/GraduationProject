<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mary.graduationproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SearchActivity"></activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:label="登陆"></activity>
        <activity android:name=".Activity.bookRoomActivity" />
        <activity
            android:name=".Activity.WebViewActivity"
            android:label="@string/title_activity_web_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.editNameActivity" />
        <activity android:name=".Activity.HouseEcaluationActivity" />
        <activity android:name=".Activity.HouseLocationActivity" />
        <activity android:name=".Activity.HouseintroductionActivity" />
        <activity android:name=".Activity.DetailsPageActivity" />
        <activity android:name=".Activity.PersonalinformationActivity" />
        <activity
            android:name=".ui.SplashActivity"
            android:theme="@style/NoActivityFullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 主页 -->
        <!-- 闪屏页 -->
        <activity android:name=".ui.GuideActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>